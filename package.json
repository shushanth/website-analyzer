{
  "name": "website-analyzer",
  "version": "1.0.0",
  "description": "website analyzer app, which analyzes the reqested websites based on the URL and shows the results",
  "main": "index.js",
  "scripts": {
    "build": "webpack --config client/config/webpack.dev.js --progress",
    "build:prod": "webpack --config client/config/webpack.prod.js --progress",
    "start": "concurrently \" npm run lint \" \"webpack --watch --colors\" \"nodemon server/server.js\"",
    "debug": "nodemon --inspect server/server.js",
    "test": "karma start client/config/karma.conf.js",
    "test:headless": "karma start client/config/karma.conf.js --browsers PhantomJS --autoWatch=true --singleRun=false --verbose",
    "lint": "npm run tslint \"client/**/*.ts\"",
    "tslint": "tslint"
  },
  "dependencies": {
    "@angular/common": "~4.0.0",
    "@angular/compiler": "~4.0.0",
    "@angular/core": "~4.0.0",
    "@angular/forms": "~4.0.0",
    "@angular/http": "~4.0.0",
    "@angular/platform-browser": "~4.0.0",
    "@angular/platform-browser-dynamic": "~4.0.0",
    "@angular/router": "~4.0.0",
    "@types/node": "^8.0.22",
    "angular-in-memory-web-api": "~0.3.0",
    "angular2-router-loader": "^0.3.5",
    "body-parser": "^1.17.2",
    "cheerio": "^1.0.0-rc.2",
    "core-js": "^2.4.1",
    "css-loader": "^0.28.11",
    "express": "^4.15.3",
    "morgan": "^1.8.2",
    "node-sass": "^4.8.3",
    "request": "^2.85.0",
    "request-promise": "^4.2.2",
    "rxjs": "5.0.1",
    "sass-loader": "^7.0.1",
    "systemjs": "0.19.40",
    "zone.js": "^0.8.4"
  },
  "devDependencies": {
    "@types/jasmine": "^2.5.36",
    "@types/node": "^6.0.45",
    "angular2-template-loader": "^0.6.2",
    "awesome-typescript-loader": "^3.2.2",
    "concurrently": "^3.5.0",
    "extract-text-webpack-plugin": "^3.0.0",
    "html-loader": "^0.5.0",
    "html-webpack-plugin": "^2.29.0",
    "jasmine-core": "^2.4.1",
    "karma": "^1.2.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-jasmine": "^1.1.1",
    "karma-jasmine-html-reporter": "0.2.2",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^2.0.13",
    "karma-phantomjs-launcher": "^1.0.2",
    "phantomjs-prebuilt": "^2.1.7",
    "nodemon": "^1.11.0",
    "raw-loader": "^0.5.1",
    "style-loader": "^0.20.3",
    "ts-loader": "^2.3.2",
    "typescript": "2.4.0",
    "webpack": "^3.4.1",
    "webpack-merge": "^4.1.2",
    "tslint": "^3.15.1",
    "tslint-loader": "^2.1.5"
  },
  "keywords": [
    "website",
    "analyzer",
    "angular",
    "webpack",
    "expressjs",
    "nodejs",
    "app"
  ],
  "author": "shushanth pallegar"
}
